{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Product",
    "desciption": "A product from Acme's catalog",
    "type": "object",
    "properties": {
        "Product": {
            "type": "object",
            "properties": {
                "ProductID": {
                    "type": "string"
                },
                "ProductName": {
                    "type": "string"
                 },
                 "Price": {
                     "type": "string"
                 },
                 "Provider": {
                    "type": "object",
                    "properties": {
                        "Name": {
                            "type": "string"
                        },
                        "City": {
                            "type": "string"
                        }
                },
                "required": [
                    "Name",
                    "City"
                ]
            }
        }
    }
}
}
